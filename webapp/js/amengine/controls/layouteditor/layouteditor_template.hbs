{{#if IS_VISIBLE }}
<table>

        <thead>
        <tr class="ui-bar-b"><th class="ui-corner-top-left">Available Items</th><th>Drop Area</th></tr>
        </thead>
    <tbody>
        <tr>
            <td style='width: 25%; vertical-align: top' class="ui-body-a ui-shadow-inset ui-input-text">
                    <ul style='min-height: 200px; padding-left: 5px; padding-right: 5px;'>
                        {{#each CONTROL_LAYOUT_EDITOR_ITEM_AVAILABLE_ITEMS }}
                            {{partial "availablepartial" this}}
                        {{/each}}
                    </ul>
            </td>
            <td style='width:75%; vertical-align: top' class="ui-body-a ui-shadow-inset ui-input-text">
                    <ul style='min-height: 200px; padding-left: 5px; padding-right: 5px;'>
                        {{#each CONTROL_LAYOUT_EDITOR_ITEM_CHILD_ITEMS }}
                            {{partial "droppartial" this}}

                        {{/each}}
                    </ul>
            </td>
        </tr>
    </tbody>


</table>
{{/if}}